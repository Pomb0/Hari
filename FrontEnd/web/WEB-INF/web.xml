<?xml version="1.0" encoding="UTF-8"?>
<web-app>
    <servlet>
        <servlet-name>Index</servlet-name>
        <servlet-class>Servlets.Index</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Background</servlet-name>
        <servlet-class>Servlets.Background</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>Standard</servlet-name>
        <servlet-class>Servlets.Standard</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>Personal</servlet-name>
        <servlet-class>Servlets.Personal</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Index</servlet-name>
        <url-pattern>/index</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>Background</servlet-name>
        <url-pattern>/background</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>Standard</servlet-name>
        <url-pattern>/standard</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>Personal</servlet-name>
        <url-pattern>/personal</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>index</welcome-file>
    </welcome-file-list>


</web-app>